<form method="post" action="{{ path('exemplar_delete', {'id': exemplar.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ exemplar.id) }}">
    <button class="btn">Delete</button>
</form>
