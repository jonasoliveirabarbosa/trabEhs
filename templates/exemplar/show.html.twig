{% extends 'base.html.twig' %}

{% block title %}Exemplar{% endblock %}

{% block content %}
    <h1>Exemplar</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ exemplar.id }}</td>
            </tr>
            <tr>
                <th>Edicao</th>
                <td>{{ exemplar.edicao }}</td>
            </tr>
            <tr>
                <th>Ano</th>
                <td>{{ exemplar.ano }}</td>
            </tr>
            <tr>
                <th>Ativo</th>
                <td>{{ exemplar.ativo ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Livre</th>
                <td>{{ exemplar.livre ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('exemplar_index') }}">back to list</a>

    <a href="{{ path('exemplar_edit', {'id': exemplar.id}) }}">edit</a>

    {{ include('exemplar/_delete_form.html.twig') }}
{% endblock %}
